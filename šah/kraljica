import java.util.Scanner;

public class Zadatak1 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println ("Unesi poziciju bele figure:");
		int bx = sc.nextInt ();
		int by = sc.nextInt ();
		System.out.println ("Unesi poziciju crne kraljice:");
		int cx = sc.nextInt ();
		int cy = sc.nextInt ();
		if (pozicijaC(bx, by, cx, cy)) {
			System.out.println ("NOMNOM");
		}
		else System.out.println ("NE");
		

	}
		static boolean pozicijaC (int bx, int by, int cx, int cy) {
			if (bx + by == cx + cy || by - bx == cy - cx) return true;
			if (cx==bx || cy==by) return true;
			
			return false;
		}
